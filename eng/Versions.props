<Project>
  <!--Package versions-->
  <PropertyGroup>
    <MicrosoftDotnetSdkInternalPackageVersion>6.0.100-preview.7.21371.16</MicrosoftDotnetSdkInternalPackageVersion>
    <MicrosoftNETILLinkTasksPackageVersion>6.0.100-preview.6.21365.1</MicrosoftNETILLinkTasksPackageVersion>
    <MicrosoftDotNetApiCompatPackageVersion>5.0.0-beta.20181.7</MicrosoftDotNetApiCompatPackageVersion>
    <MicrosoftDotNetBuildTasksFeedPackageVersion>6.0.0-beta.21212.6</MicrosoftDotNetBuildTasksFeedPackageVersion>
    <MicrosoftNETCoreAppRefPackageVersion>6.0.0-preview.7.21370.18</MicrosoftNETCoreAppRefPackageVersion>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Trim all characters after first `-` or `+` is encountered. -->
    <DotNetPreviewVersionBand Condition=" '$(DotNetPreviewVersionBand)' == '' ">$([System.Text.RegularExpressions.Regex]::Replace($(MicrosoftDotnetSdkInternalPackageVersion), `[-+].*$`, ""))</DotNetPreviewVersionBand>
  </PropertyGroup>
</Project>
