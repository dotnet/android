  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\TestRunner.Core\TestRunner.Core.NET.csproj" />
    <ProjectReference Include="..\..\src\Xamarin.Android.NUnitLite\Xamarin.Android.NUnitLite.NET.csproj" />
  </ItemGroup>
</Project>
