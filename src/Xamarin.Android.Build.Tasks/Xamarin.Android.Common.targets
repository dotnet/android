</Target>

<Target Name="_Uninstall">
  <Exec Command="&quot;$(AdbToolPath)adb&quot; $(AdbTarget) uninstall $(_AndroidPackage)" />
</Target>

<Target Name="Uninstall"
    Condition="'$(AndroidApplication)'!='' And $(AndroidApplication)"
