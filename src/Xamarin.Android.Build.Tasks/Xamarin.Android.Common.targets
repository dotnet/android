	</CreateItem>
	<CreateItem Include="$(_AndroidResgenFlagFile)">
		<Output TaskParameter="Include" ItemName="FileWrites"/>
	</CreateItem>
	<CreateItem Include="$(IntermediateOutputPath)R.txt">
