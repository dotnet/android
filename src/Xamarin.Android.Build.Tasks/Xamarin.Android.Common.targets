      IsApplication="$(AndroidApplication)"
      JavaSourceFiles="@(AndroidJavaSource)"
      JavaLibraries="@(AndroidJavaLibrary)"
      NativeLibraries="@(AndroidNativeLibrary)"
      EmbeddedNativeLibraries="@(EmbeddedNativeLibrary)" />
