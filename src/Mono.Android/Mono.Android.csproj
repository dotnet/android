        SourceFiles="$(OutputPath)ref\Mono.Android.dll;$(JavaInteropSourceDirectory)\bin\$(Configuration)-net7.0\ref\Java.Interop.dll"
        DestinationFolder="$(MicrosoftAndroidRefPackDir)"
        SkipUnchangedFiles="true"
