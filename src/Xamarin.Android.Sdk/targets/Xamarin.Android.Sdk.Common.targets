<Target Name="_GetMonoPlatformJarPath">
  <GetMonoPlatformJar TargetFrameworkDirectory="$(TargetFrameworkDirectory)">
    <Output TaskParameter="MonoPlatformJarPath" PropertyName="MonoPlatformJarPath" />
